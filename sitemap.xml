---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9 http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd" xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">    
  <url>
    <loc>http://www.alexlande.com/</loc>
    <changefreq>monthly</changefreq>
    <priority>1.0</priority>
  </url>
  <url>
    <loc>http://www.alexlande.com/about/</loc>
    <changefreq>yearly</changefreq>
  </url>
  <url>
    <loc>http://www.alexlande.com/projects/</loc>
    <changefreq>monthly</changefreq>
  </url>
  <url>
    <loc>http://www.alexlande.com/articles/</loc>
    <changefreq>monthly</changefreq>
  </url>
  {% for post in site.categories.articles limit:20 %}
  <url>
    <loc>http://www.alexlande.com/articles{{ post.url }}/</loc>
    <lastmod>{{ post.date | date_to_xmlschema }}</lastmod>
    <changefreq>monthly</changefreq>
    <priority>0.8</priority>
  </url>
  {% endfor %}
    {% for post in site.categories.projects limit:20 %}
  <url>
    <loc>http://www.alexlande.com/projects{{ post.url }}/</loc>
    <lastmod>{{ post.date | date_to_xmlschema }}</lastmod>
    <changefreq>monthly</changefreq>
    <priority>0.8</priority>
  </url>
  {% endfor %}
</urlset>